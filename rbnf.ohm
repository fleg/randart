RBNF {
  Entry =  Rule*
  RuleName = #(upper | digit)+
  Rule = RuleName Branch+ ";"
  Branch = "|"+ Expr
  Expr = Unop | Binop | Triop | RuleName | Symbol
  OpName = #(lower (lower | digit)*)
  Unop = OpName "(" Expr ")"
  Binop = OpName "(" Expr "," Expr ")"
  Triop = OpName "(" Expr "," Expr "," Expr ")"
  Symbol = "x" | "y" | "t" | "random"
}
